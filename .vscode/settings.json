{
    "python.testing.pytestArgs": [
        "."
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.autoTestDiscoverOnSaveEnabled": true,
    "python.languageServer": "Pylance",
    "python.formatting.blackArgs": [
        "--line-length",
        "120"
    ],
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "editor.fontSize": 16,
    "editor.lineHeight": 1.5,
    "editor.letterSpacing": 0.5,
    "files.exclude": {
        "**/.git": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true,
        "**/.venv": true,
        "**/.ropeproject": true,
        "**/.history": true
    },
    "search.exclude": {
        "**/.git": true,
        "**/.venv": true
    },
    "highlight.decorations": {
        "rangeBehavior": 3
    },
    "highlight.regexFlags": "gi",
    "highlight.maxMatches": 25000,
    "highlight.regexes": {
        "( *[.]n ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "purple"
                },
                {
                    "color": "purple",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *[.]adj ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "orange",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *[.]prop ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "IndianRed",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *[.]com ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "#a8a8ff",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *[.]np ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "pink",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *.v ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "#D2691E",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *[.]meta ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "yellow",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *[.]table ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "DarkCyan",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *[.][a-zA-Z0-9_-]+ ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "DarkCyan",
                    "fontWeight": "bold"
                }
            ]
        },
        "( *#.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey",
                    "fontStyle": "italic"
                }
            ]
        },
        "( *- .*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "CadetBlue",
                    "fontStyle": "italic"
                }
            ]
        },
        "( *[-]  *[a-zA-Z0-9_-]+: ?) (.*\\n)": {
            "regexFlags": "g",
            "filterFileRegex": ".*\\.wen",
            "decorations": [
                {
                    "color": "grey"
                },
                {
                    "color": "brown"
                }
            ]
        }
    },    
}
